{
  "name": "like-vendor-backend",
  "version": "1.0.0",
  "description": "Like Vendor Backend",
  "main": "index.js",
  "scripts": {
    "ts:node": "ts-node-dev --files src/index.ts",
    "dev": "nodemon",
    "build": "tsc",
    "clean": "rm -rf build",
    "start": "node build/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "docker:build": "docker build -t like-vendor-backend .",
    "docker:run": "docker run -p 3000:3000 like-vendor-backend",
    "docker:compose": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "migrate": "node build/scripts/migrate.js",
    "seed": "node build/scripts/seed.js",
    "seed:data": "ts-node src/scripts/seed-database.ts",
    "seed:complete": "ts-node src/scripts/seed-complete.ts",
    "check:data": "ts-node src/scripts/check-data.ts",
    "create:admin": "npx ts-node src/scripts/create-admin.ts",
    "create:vehicle-users": "npx ts-node src/scripts/create-vehicle-users.ts",
    "create:vehicles-by-category": "npx ts-node src/scripts/create-vehicles-by-category.ts",
    "create:drivers-for-vehicles": "npx ts-node src/scripts/create-drivers-for-vehicles.ts",
    "create:business-data": "npx ts-node src/scripts/create-business-data.ts",
    "set:mixed-vehicle-status": "npx ts-node src/scripts/set-mixed-vehicle-status.ts",
    "list:users": "npx ts-node src/scripts/list-users.ts",
    "health": "curl -f http://localhost:3000/health || exit 1",
    "production:test": "node scripts/production-test.js",
    "production:validate": "set NODE_ENV=production && npm run build && npm run production:test",
    "production:start": "set NODE_ENV=production && npm start"
  },
  "keywords": [],
  "author": "lvendor",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-medialive": "^3.925.0",
    "@aws-sdk/client-mediapackage": "^3.925.0",
    "@aws-sdk/client-s3": "^3.925.0",
    "@aws-sdk/s3-request-presigner": "^3.925.0",
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-express": "^11.1.6",
    "@types/mongoose": "^5.11.97",
    "@types/node-cache": "^4.2.5",
    "@types/nodemailer": "^7.0.2",
    "@types/simple-peer": "^9.11.9",
    "axios": "^1.7.2",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.2",
    "chalk": "^3.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.1",
    "mongoose": "^8.19.0",
    "multer": "^2.0.2",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.6",
    "password-validator": "^5.1.1",
    "pdfkit": "^0.17.2",
    "peer": "^1.0.2",
    "qrcode": "^1.5.4",
