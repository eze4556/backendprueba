    "sharp": "^0.33.5",
    "simple-peer": "^9.11.1",
    "soap": "^1.4.2",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "ts-dotenv": "^0.8.4",
    "uuid": "^13.0.0",
    "winston": "^3.18.3",
    "wrtc": "^0.4.7",
    "xml2js": "^0.6.2",
    "xss-clean": "^0.1.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chalk": "^2.2.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.14.10",
    "@types/node-cron": "^3.0.11",
    "@types/pdfkit": "^0.17.3",
